{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --experimental-https",
    "postinstall": "cd ../contracts && npm install",
    "prebuild": "cd ../contracts && npm run build",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "ts-watch": "tsc --noEmit --incremental --watch",
    "deploy": "next build --no-lint && touch out/.nojekyll && node ./ghp-postbuild && git add -f out && git commit -m \"Deploy gh-pages\" && cd .. && git subtree push --prefix ui/out origin gh-pages"
  },
  "dependencies": {
    "coi-serviceworker": "^0.1.7",
    "next": "14.2.3",
    "o1js": "^1.7.0",
    "react": "^18",
    "react-currency-format": "github:mohitgupta8888/react-currency-format#4f365ec5005e23451a3b95da1a209b0025622b8d",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5"
  }
}